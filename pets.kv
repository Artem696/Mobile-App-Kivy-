                id: container_lost
                orientation: "vertical"
                adaptive_height: True
                spacing: dp(25)

<StartScreen>:
    FloatLayout:
        RoundedButton:
            pos_hint: {'x': 0.02, 'y': 0.305}
            size_hint: 0.96, 0.05
            font_size: '20sp'   
            text: 'Войти/Зарегистрироваться'   
            on_release: 
                app.root.current = "login"
        Image:
