            pos_hint: {'x': 0, 'y': -0.238}
            color: 0,0,0,1
            font_size: '20sp'
            text: 'или'
        RoundedButton:
            pos_hint: {'x': 0.02, 'y': 0.17}
            size_hint: 0.96, 0.05
            font_size: '20sp'
            text: 'Войти без регистрации'
            on_release: 
                app.root.current = "main"
                app.root.transition.direction = "left"
<MainScreen>:
    FloatLayout:
        size_hint: 1,0.05
        pos_hint: {'x':0,'y':0.95}
        canvas.before:
            Color:
                rgba: (0.34,0.71,0.98,1)
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            pos_hint: {'x':0,'y':0}
            color: 0,0,0,1
            font_size: sp(18)
            text:'Потеряшка'
        MDIconButton:
            icon: 'filter'
            ripple_scale: 0.8
            pos_hint: {'x': 0.93, 'y': -0.2}
            user_font_size: sp(35)
            on_release:
                app.root.current = 'lost_filter'
                app.root.transition.direction = "left"
    RelativeLayout:
        size_hint: 1,0.88
        pos_hint: {'x':0,'y':0.07}
        MDTabs:
            tab_hint_x: True
            allow_stretch: True
            background_color: 1,1,1,1
            text_color_normal: 0,0,0,1
            text_color_active: 0,0,0,1
            indicator_color: 0.34,0.71,0.98,1
            lock_swiping: True
            Lost_map:
                icon: 'map-marker'
                title: 'Карта'
            Lost_list:
                id: lost_list
                icon: 'format-list-bulleted'
                title: 'Список'   
    NavigationBar:
        size_hint: 1,0.07
        pos_hint: {'x': 0, 'y': 0}
        #elevation: 20
        md_bg_color: 1,1,1,1
        canvas:
            Color:
                rgba: (0,0,0,0.5)
            Line:
                points: self.x, self.y+self.height, self.x+self.width, self.y+self.height
        #MDFloatLayout:

    MDIconButton:
        id: nav_icon1
        icon: 'paw'
        ripple_scale: 0.8
        pos_hint: {'x': 0.05, 'y': 0.01}
        user_font_size: sp(36)
        theme_text_color: 'Custom'
        text_color: 0.34,0.71,0.98,1
        on_release:
            #app.root.transition = NoTransition()
            app.change_color(self)
            app.root.current = 'main'
    MDIconButton:
        id: nav_icon2
        icon: 'clipboard-check-outline'
        ripple_scale: 0.8
        pos_hint: {'x': 0.25, 'y': 0.01}
        user_font_size: dp(36)
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
        on_release:
            app.change_color(self)
            app.root.current = 'service_list'
    MDIconButton:
        id: nav_icon3
        icon: 'plus-circle-outline'
        ripple_scale: 0.8
        pos_hint: {'x': 0.45, 'y': 0.01}
        user_font_size: dp(36)
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
        on_release:
            app.change_color(self)
            app.root.current = 'add'
    MDIconButton:
        id: nav_icon4
        icon: 'bell-outline'
        ripple_scale: 0.8
        pos_hint: {'x': 0.65, 'y': 0.01}
        user_font_size: dp(36)
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
        on_release:
            app.change_color(self)
            app.transition_notif()
    MDIconButton:
        id: nav_icon5
