<MainScreen>:
    FloatLayout:
        size_hint: 1,0.05
        pos_hint: {'x':0,'y':0.95}
        canvas.before:
            Color:
                rgba: (0.34,0.71,0.98,1)
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            pos_hint: {'x':0,'y':0}
            color: 0,0,0,1
            font_size: sp(18)
            text:'Потеряшка'
        MDIconButton:
            icon: 'filter'
            ripple_scale: 0.8
            pos_hint: {'x': 0.89, 'y': -0.2}
            user_font_size: sp(35)
            on_release:
                app.root.current = 'lost_filter'
                app.root.transition.direction = "left"
    RelativeLayout:
        size_hint: 1,0.88
        pos_hint: {'x':0,'y':0.07}
        MDTabs:
            id: tabs
            tab_hint_x: True
            allow_stretch: True
            background_color: 1,1,1,1
            text_color_normal: 0,0,0,1
            text_color_active: 0,0,0,1
            indicator_color: 0.34,0.71,0.98,1
            lock_swiping: True
            Lost_map:
                id: lost_map
                icon: 'map-marker'
                title: 'Карта'
            Lost_list:
                id: lost_list
                icon: 'format-list-bulleted'
                title: 'Список'   
    NavigationBar:
        size_hint: 1,0.07
        pos_hint: {'x': 0, 'y': 0}
        #elevation: 20
        md_bg_color: 1,1,1,1
        canvas:
            Color:
                rgba: (0,0,0,0.5)
            Line:
                points: self.x, self.y+self.height, self.x+self.width, self.y+self.height
        #MDFloatLayout:

        MDIconButton:
            id: nav_icon1
            icon: 'paw'
            ripple_scale: 0.8
            pos_hint: {'x': 0.05, 'y': 0.01}
            user_font_size: sp(36)
            theme_text_color: 'Custom'
            text_color: 0.34,0.71,0.98,1
            on_release:
                #app.root.transition = NoTransition()
                app.change_color(self)
                app.root.current = 'main'
        MDIconButton:
            id: nav_icon2
            icon: 'clipboard-check-outline'
            ripple_scale: 0.8
            pos_hint: {'x': 0.25, 'y': 0.01}
            user_font_size: dp(36)
            theme_text_color: 'Custom'
            text_color: 0,0,0,1
            on_release:
                app.change_color(self)
                app.root.current = 'service_list'
        MDIconButton:
            id: nav_icon3
            icon: 'plus-circle-outline'
            ripple_scale: 0.8
            pos_hint: {'x': 0.45, 'y': 0.01}
            user_font_size: dp(36)
            theme_text_color: 'Custom'
            text_color: 0,0,0,1
            on_release:
                app.change_color(self)
                app.root.current = 'add'
        MDIconButton:
            id: nav_icon4
            icon: 'bell-outline'
            ripple_scale: 0.8
            pos_hint: {'x': 0.65, 'y': 0.01}
            user_font_size: dp(36)
            theme_text_color: 'Custom'
            text_color: 0,0,0,1
            on_release:
                app.change_color(self)
                app.transition_notif()
        MDIconButton:
            id: nav_icon5
            icon: 'account-circle-outline'
            ripple_scale: 0.8
            pos_hint: {'x': 0.84, 'y': 0.01}
            user_font_size: dp(36)
            theme_text_color: 'Custom'
            text_color: 0,0,0,1
            on_release:
                app.change_color(self)
                app.transition_profile()
<Sign_in>:
    BoxLayout:
        orientation: 'vertical'
        size_hint_x: 0.9
        pos_hint: {'x': 0.05, 'y': 0.8}
        spacing: '10dp'
        MDTextField:
            id: email_sign
            hint_text: "Email"
            mode: "line"
        MDTextField:
            id: passw_sign
            hint_text: "Пароль"
            mode: "line"
            password: True
            password_mask: '•'
    MDIconButton:
        icon: 'eye-off'
        pos_hint: {'x': 0.9, 'y': 0.81}
        user_font_size: dp(24)
        on_release: root.ids.passw_sign.password = False if root.ids.passw_sign.password else True
        on_release: self.icon = 'eye' if root.ids.passw_sign.password else 'eye-off'
    RoundedButton:
        pos_hint: {'x': 0.02, 'y': 0.025}
        size_hint: 0.96, 0.05
        font_size: '20sp'
        color: 0,0,0,1
        text: 'Войти'
        on_release: 
            root.add_markers() 
            root.get_text_sign()
<Agreement@ButtonBehavior+Label>
<Registration>:
    BoxLayout:
        orientation: 'vertical'
        size_hint_x: 0.9
        pos_hint: {'center_x': 0.5, 'center_y': 0.84}
        spacing: '10dp'
        MDTextField:
            id: name_reg
            hint_text: root.hint
            mode: "line"
            set_text: root.check_text
            required: True
            helper_text_mode: "on_error"
            helper_text: "Это поле должно быть заполнено"
        MDTextField:
            id: lname_reg
            hint_text: "Фамилия"
            mode: "line"
            required: True
            helper_text_mode: "on_error"
            helper_text: "Это поле должно быть заполнено"
            set_text: root.check_text
        MDTextField:
            id: email_reg
            hint_text: "Email"
            mode: "line"
            required: True
            helper_text_mode: "on_error"
            helper_text: "Это поле должно быть заполнено"
            set_text: root.check_text
        # MDTextField:
        #     id: city_reg
        #     hint_text: "Город"
        #     mode: "line"
        #     required: True
        #     helper_text_mode: "on_error"
        #     helper_text: "Это поле должно быть заполнено"
        #     set_text: root.check_text
        MDTextField:
            id: phone_reg
            hint_text: "Мобильный телефон"
            mode: "line"
            input_filter: 'int'
            required: True
            helper_text_mode: "on_error"
            helper_text: "Это поле должно быть заполнено"
            set_text: root.check_text
        MDTextField:
            id: passw_reg1
            hint_text: "Пароль"
            mode: "line"
            password: True
            password_mask: '•'
            set_text: root.check_text
        MDTextField:
            id: passw_reg2
            hint_text: "Повторите пароль"
            mode: "line"
            password: True
            password_mask: '•'
    Label:
        id: pas_mes
        font_size: '14sp'
        color: 1,0,0,1
        text: 'Пароли не совпадают'
        pos_hint: {'center_x': 0.5, 'center_y': 0.15}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_name
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод имени'
        pos_hint: {'center_x': 0.147, 'center_y': 0.875}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_lname
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод фамилии'
        pos_hint: {'center_x': 0.155, 'center_y': 0.79}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_email
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод email'
        pos_hint: {'center_x': 0.145, 'center_y': 0.7}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_city
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод города'
        pos_hint: {'center_x': 0.15, 'center_y': 0.615}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_phone
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод мобильного телефона'
        pos_hint: {'center_x': 0.202, 'center_y': 0.525}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: requirements_pass
        font_size: '11sp'
        color: 0,0,0,1
        text: 'Пароль должен содержать хотя бы одно число,\nхотя бы одну латинскую букву в нижнем и верхнем регистре,\nможет содержать @#$%-_.\nДлина составляет от 6 до 20 символов'
        pos_hint: {'center_x': 0.34, 'center_y': 0.32}
        disabled: False
    MDIconButton:
        icon: 'eye-off'
        pos_hint: {'x': 0.9, 'y': 0.448}
        user_font_size: dp(24)
        on_release: root.ids.passw_reg1.password = False if root.ids.passw_reg1.password else True
        on_release: self.icon = 'eye' if root.ids.passw_reg1.password else 'eye-off'
    MDIconButton:
        icon: 'eye-off'
        pos_hint: {'x': 0.9, 'y': 0.36}
        user_font_size: dp(24)
        on_release: root.ids.passw_reg2.password = False if root.ids.passw_reg2.password else True
        on_release: self.icon = 'eye' if root.ids.passw_reg2.password else 'eye-off'
    RoundedButton:
        id: btn_reg
        pos_hint: {'x': 0.02, 'y': 0.05}
        size_hint: 0.96, 0.05
        font_size: '20sp'
        color: 0,0,0,1
        text: 'Зарегистрироваться'
        on_release:
            root.add_markers()  
            root.check_field()
    Agreement:
        pos_hint: {'center_x': 0.5, 'center_y': 0.02}
        size_hint_y: 0.01
        font_size: '12sp'
        color: 0.22,0.42,1,1
        markup: True
        text:'Нажимая на кнопку "Зарегистрироваться" вы даёте согласие на [u]обработку персоналльных данных[/u]'
        on_press:
            import webbrowser
            webbrowser.open('https://getfile.dokpub.com/yandex/get/https://disk.yandex.ru/i/UvunXBkVPxGAJA')
