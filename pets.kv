<RoundedButton@Button>:
    background_color: 0,0,0,0
    canvas.before:
        Color:
            rgba: (0.34,0.71,0.98,1) if self.state=='normal' else (0,.7,.7,1)  # visual feedback of press
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: [20]
<PopupMarker>:
    BoxLayout:
        orientation: 'vertical'
        canvas.before:
            Color:
                rgba: (1,1,1,1)
            RoundedRectangle:
                pos: self.pos
                size: self.size
                radius: [20]
        Image:
            id: img_marker
            source: 'Image\\logo.png'
            size_hint: 1, 1
        Label:
            id: type_marker
            color: 0,0,0,1
            font_size: '15sp'
            text: 'Пропала'
<Lost_card>:
    size_hint_y: None
    height: 200
    size_hint_x: None
    width: 500
    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
    canvas:
        Color:
            rgba: (0,0,0,1)
        Line:
            rectangle: self.x, self.y, self.width, self.height
    Image:
        id: card_image
        source: 'Image\\catt.jpg'
        size_hint_y: 0.9
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
    BoxLayout:
        orientation: 'vertical'
        size_hint: 1,0.4
        spacing: dp(25)
        pos_hint: {'center_x': .5, 'center_y': .5}
        FloatLayout:
            size_hint: 1,0.5
            Label:
                id: date
                color: 0,0,0,1
                font_size: '15sp'
                text: 'Дата/Время'
                opacity: 0
            # MDIconButton:
            #     id: btn_close
            #     icon: 'pencil'
            #     ripple_scale: 0.1
            #     user_font_size: sp(16)
            #     pos_hint: {'center_x': 0.8, 'y': 0.9}
            #     opacity: 0
            #     on_press: root.change(self)
            MDIconButton:
                id: btn_close
                icon: 'window-close'
                ripple_scale: 0.1
                user_font_size: sp(16)
                pos_hint: {'center_x': 0.9, 'y': 0.9}
                opacity: 0
                on_press: root.delete_ad(self)
        Label:
            id: card_date
            color: 0,0,0,1
            font_size: '15sp'
            text: 'Дата/Время'
        Label:
            id: gender
            color: 0,0,0,1
            font_size: '15sp'
            text: 'Мальчик'
        Label:
            id: type
            color: 0,0,0,1
            font_size: '15sp'
            text: 'Пропали'
        Label:
            id: city
            color: 0,0,0,1
            font_size: '15sp'
            text: 'Город'
<Lost_map>:
    MapView:
        id: map
        lat:55.818
        lon:37.3317
        zoom:14
        pos_hint: {'x': 0, 'y': 0}
        size_hint: 1, 1
        map_source: 'osm'
<Lost_list>:
    BoxLayout:
        orientation: "vertical"
        ScrollView:
            do_scroll_x: False
