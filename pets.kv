        Label:
            pos_hint: {'x': 0, 'y': -0.238}
            color: 0,0,0,1
            font_size: '20sp'
            text: 'или'
        RoundedButton:
            pos_hint: {'x': 0.02, 'y': 0.17}
            size_hint: 0.96, 0.05
            font_size: '20sp'
