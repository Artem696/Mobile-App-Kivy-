WindowManager:
    StartScreen:
        name: 'start'
        id: startscreen
    MainScreen:
        name: "main"
        id: mainscreen
    LoginScreen:
        name: 'login'
        id: loginscreen
    Notification_NoReg:
        name: 'notif_noreg'
        id: notification_noreg
    Profile_NoReg:
        name: 'profile_noreg'
        id: profile_noreg
    Lost_Filter:
        name: 'lost_filter'
        id: lost_filter
    Ad_Screen:
        name: 'ad'
        id: ad_screen
    Service_List_Screen:
        name: 'service_list'
        id: service_list_screen
    Service_Screen:
        name: 'service'
        id: service_screen
    Service_Filter:
        name: 'service_filter'
        id: service_filter
    Profile_Screen:
        name: 'profile'
        id: profile_screen
    Personal_Data:
        name: 'data_personal'
        id: personal_data
    Change_Password_Screen:
        name: 'change_password'
        id: change_password_Screen
    Add_Screen:
        name: 'add'
        id: add_screen
    Add_See_Screen:
        name: 'add_see'
        id: add_see_screen
    Add_Lost_Screen:
        name: 'add_lost'
        id: add_lost_screen
    Add_Overexposure_Screen:
        name: 'add_overexposure'
        id: add_overexposure_screen
    Add_Nanny_Screen:
        name: 'add_nanny'
        id: add_nanny_screen
    Add_Walk_Screen:
        name: 'add_walk'
        id: add_walk_screen
<RoundedButton@Button>:
    background_color: 0,0,0,0
    canvas.before:
        Color:
            rgba: (0.34,0.71,0.98,1) if self.state=='normal' else (0,.7,.7,1)  # visual feedback of press
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: [20]
<PopupMarker>:
    BoxLayout:
        orientation: 'vertical'
        canvas.before:
            Color:
                rgba: (1,1,1,1)
            RoundedRectangle:
                pos: self.pos
                size: self.size
                radius: [20]
        Image:
            source: 'Image\\cat.jpg'
            size_hint: 1, 1
        Label:
            color: 0,0,0,1
            font_size: '15sp'
            text: 'Мальчик'
<Lost_card>:
    size_hint_y: None
    height: 200
    size_hint_x: None
    width: 500
    pos_hint: {'center_x': .5, 'center_y': .5}
    canvas:
        Color:
            rgba: (0,0,0,1)
        Line:
            rectangle: self.x, self.y, self.width, self.height
    Image:
        id: card_image
        source: 'Image\\cat.jpg'
    BoxLayout:
        orientation: 'vertical'
        size_hint: 1,0.3
        spacing: dp(10)
        pos_hint: {'center_x': .5, 'center_y': .45}
        Label:
            id: card_date
            color: 0,0,0,1
            font_size: '15sp'
            text: 'Дата/Время'
        Label:
            id: gender
            color: 0,0,0,1
            font_size: '15sp'
            text: 'Мальчик'
        Label:
            id: type
            color: 0,0,0,1
            font_size: '15sp'
            text: 'Пропали'
            disabled: True
<Lost_map>:
    MapView:
        lat:55.818
        lon:37.3317
        zoom:10
        pos_hint: {'x': 0, 'y': 0}
        size_hint: 1, 1
        map_source: "osm"
        get_lon: root.get_lon(self.zoom,self.x)
        MapMarkerPopup:
            source: 'Image\\marker.png'
            lat:55.818
            lon:37.3317
            PopupMarker:
                size_hint: None, None
                size: 200, 200
<Lost_list>:
    BoxLayout:
        orientation: "vertical"
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            bar_color: [0, 0, 0, 1]
            MDBoxLayout:
                id: container_lost
                orientation: "vertical"
                adaptive_height: True
                spacing:dp(25)

<StartScreen>:
    FloatLayout:
        RoundedButton:
            pos_hint: {'x': 0.02, 'y': 0.305}
            size_hint: 0.96, 0.05
            font_size: '20sp'   
            text: 'Войти/Зарегистрироваться'   
            on_release: 
                app.root.current = "login"
                app.root.transition.direction = "left"
        Image:
            source: 'Image\\logo.png'
            pos_hint: {'x': 0.2, 'y': 0.4}
            size_hint: 0.6, 0.6
        Label:
            pos_hint: {'x': 0, 'y': -0.238}
            color: 0,0,0,1
            font_size: '20sp'
            text: 'или'
        RoundedButton:
            pos_hint: {'x': 0.02, 'y': 0.17}
            size_hint: 0.96, 0.05
            font_size: '20sp'
            text: 'Войти без регистрации'
            on_release: 
                app.root.current = "main"
                app.root.transition.direction = "left"
<MainScreen>:
    FloatLayout:
        size_hint: 1,0.05
        pos_hint: {'x':0,'y':0.95}
        canvas.before:
            Color:
                rgba: (0.34,0.71,0.98,1)
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            pos_hint: {'x':0,'y':0}
            color: 0,0,0,1
            font_size: sp(18)
            text:'Потеряшка'
        MDIconButton:
            icon: 'filter'
            ripple_scale: 0.8
            pos_hint: {'x': 0.93, 'y': -0.2}
            user_font_size: sp(35)
            on_release:
                app.root.current = 'lost_filter'
                app.root.transition.direction = "left"
    RelativeLayout:
        size_hint: 1,0.88
        pos_hint: {'x':0,'y':0.07}
        MDTabs:
            id: tabs
            tab_hint_x: True
            allow_stretch: True
            background_color: 1,1,1,1
            text_color_normal: 0,0,0,1
            text_color_active: 0,0,0,1
            indicator_color: 0.34,0.71,0.98,1
            lock_swiping: True
            Lost_map:
                id: lost_map
                icon: 'map-marker'
                title: 'Карта'
            Lost_list:
                id: lost_list
                icon: 'format-list-bulleted'
                title: 'Список'   
    NavigationBar:
        size_hint: 1,0.07
        pos_hint: {'x': 0, 'y': 0}
        #elevation: 20
        md_bg_color: 1,1,1,1
        canvas:
            Color:
                rgba: (0,0,0,0.5)
            Line:
                points: self.x, self.y+self.height, self.x+self.width, self.y+self.height
        #MDFloatLayout:

    MDIconButton:
        id: nav_icon1
        icon: 'paw'
        ripple_scale: 0.8
        pos_hint: {'x': 0.05, 'y': 0.01}
        user_font_size: sp(36)
        theme_text_color: 'Custom'
        text_color: 0.34,0.71,0.98,1
        on_release:
            #app.root.transition = NoTransition()
            app.change_color(self)
            app.root.current = 'main'
    MDIconButton:
        id: nav_icon2
        icon: 'clipboard-check-outline'
        ripple_scale: 0.8
        pos_hint: {'x': 0.25, 'y': 0.01}
        user_font_size: dp(36)
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
        on_release:
            app.change_color(self)
            app.root.current = 'service_list'
    MDIconButton:
        id: nav_icon3
        icon: 'plus-circle-outline'
        ripple_scale: 0.8
        pos_hint: {'x': 0.45, 'y': 0.01}
        user_font_size: dp(36)
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
        on_release:
            app.change_color(self)
            app.root.current = 'add'
    MDIconButton:
        id: nav_icon4
        icon: 'bell-outline'
        ripple_scale: 0.8
        pos_hint: {'x': 0.65, 'y': 0.01}
        user_font_size: dp(36)
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
        on_release:
            app.change_color(self)
            app.transition_notif()
    MDIconButton:
        id: nav_icon5
        icon: 'account-circle-outline'
        ripple_scale: 0.8
        pos_hint: {'x': 0.84, 'y': 0.01}
        user_font_size: dp(36)
        theme_text_color: 'Custom'
        text_color: 0,0,0,1
        on_release:
            app.change_color(self)
            app.transition_profile()
<Sign_in>:
    BoxLayout:
        orientation: 'vertical'
        size_hint_x: 0.9
        pos_hint: {'x': 0.05, 'y': 0.8}
        spacing: '10dp'
        MDTextField:
            id: email_sign
            hint_text: "Email"
            mode: "rectangle"
        MDTextField:
            id: passw_sign
            hint_text: "Пароль"
            mode: "rectangle"
            password: True
            password_mask: '•'
    MDIconButton:
        icon: 'eye-off'
        pos_hint: {'x': 0.9, 'y': 0.798}
        user_font_size: dp(24)
        on_release: root.ids.passw_sign.password = False if root.ids.passw_sign.password else True
        on_release: self.icon = 'eye' if root.ids.passw_sign.password else 'eye-off'
    RoundedButton:
        pos_hint: {'x': 0.02, 'y': 0.025}
        size_hint: 0.96, 0.05
        font_size: '20sp'
        color: 0,0,0,1
        text: 'Войти'
        on_release: root.get_text_sign()
<Agreement@ButtonBehavior+Label>
<Registration>:
    BoxLayout:
        orientation: 'vertical'
        size_hint_x: 0.9
        pos_hint: {'center_x': 0.5, 'center_y': 0.855}
        spacing: '10dp'
        MDTextField:
            id: name_reg
            hint_text: root.hint
            mode: "line"
            set_text: root.check_text
            required: True
            helper_text_mode: "on_error"
            helper_text: "Это поле должно быть заполнено"
        MDTextField:
            id: lname_reg
            hint_text: "Фамилия"
            mode: "line"
            required: True
            helper_text_mode: "on_error"
            helper_text: "Это поле должно быть заполнено"
            set_text: root.check_text
        MDTextField:
            id: email_reg
            hint_text: "Email"
            mode: "line"
            required: True
            helper_text_mode: "on_error"
            helper_text: "Это поле должно быть заполнено"
            set_text: root.check_text
        MDTextField:
            id: city_reg
            hint_text: "Город"
            mode: "line"
            required: True
            helper_text_mode: "on_error"
            helper_text: "Это поле должно быть заполнено"
            set_text: root.check_text
        MDTextField:
            id: phone_reg
            hint_text: "Мобильный телефон"
            mode: "line"
            input_filter: 'int'
            required: True
            helper_text_mode: "on_error"
            helper_text: "Это поле должно быть заполнено"
            set_text: root.check_text
        MDTextField:
            id: passw_reg1
            hint_text: "Пароль"
            mode: "line"
            password: True
            password_mask: '•'
            set_text: root.check_text
        MDTextField:
            id: passw_reg2
            hint_text: "Повторите пароль"
            mode: "line"
            password: True
            password_mask: '•'
    Label:
        id: pas_mes
        font_size: '14sp'
        color: 1,0,0,1
        text: 'Пароли не совпадают'
        pos_hint: {'center_x': 0.5, 'center_y': 0.15}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_name
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод имени'
        pos_hint: {'center_x': 0.147, 'center_y': 0.875}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_lname
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод фамилии'
        pos_hint: {'center_x': 0.155, 'center_y': 0.79}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_email
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод email'
        pos_hint: {'center_x': 0.145, 'center_y': 0.7}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_city
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод города'
        pos_hint: {'center_x': 0.15, 'center_y': 0.615}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: error_phone
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Неккоректный ввод мобильного телефона'
        pos_hint: {'center_x': 0.202, 'center_y': 0.525}
        size_hint_y: 0.01
        disabled: True
    Label:
        id: requirements_pass
        font_size: '11sp'
        color: 1,0,0,1
        text: 'Пароль должен содержать хотя бы одно число,\nхотя бы одну латинскую букву в нижнем и верхнем регистре регистре,\nможет содержать @#$%-_.\nДлина составляет от 6 до 20 символов'
        pos_hint: {'center_x': 0.305, 'center_y': 0.32}
        disabled: False
    MDIconButton:
        icon: 'eye-off'
        pos_hint: {'x': 0.9, 'y': 0.448}
        user_font_size: dp(24)
        on_release: root.ids.passw_reg1.password = False if root.ids.passw_reg1.password else True
        on_release: self.icon = 'eye' if root.ids.passw_reg1.password else 'eye-off'
    MDIconButton:
        icon: 'eye-off'
        pos_hint: {'x': 0.9, 'y': 0.36}
        user_font_size: dp(24)
        on_release: root.ids.passw_reg2.password = False if root.ids.passw_reg2.password else True
        on_release: self.icon = 'eye' if root.ids.passw_reg2.password else 'eye-off'
    RoundedButton:
        id: btn_reg
        pos_hint: {'x': 0.02, 'y': 0.05}
        size_hint: 0.96, 0.05
        font_size: '20sp'
        color: 0,0,0,1
        text: 'Зарегистрироваться'
        on_release: root.check_field()
    Agreement:
        pos_hint: {'center_x': 0.5, 'center_y': 0.02}
        size_hint_y: 0.01
        font_size: '12sp'
        color: 0.22,0.42,1,1
        markup: True
        text:'Нажимая на кнопку "Зарегистрироваться" вы даёте согласие на [u]обработку персоналльных данных[/u]'
        on_press:
            import webbrowser
            webbrowser.open('https://getfile.dokpub.com/yandex/get/https://disk.yandex.ru/i/UvunXBkVPxGAJA')
